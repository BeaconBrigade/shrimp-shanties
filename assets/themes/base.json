{
  "#fira-sans-bold": {
    "font": {
      "name": "FiraSans-Bold",
      "regular_path": "../assets/fonts/FiraSans-Bold.ttf"
    }
  },
  "#title": {
    "prototype": "#fira-sans-bold",
    "font": {
      "size": "50"
    },
    "colours": {
      "normal_text": "rgb(235, 247, 238)"
    }
  },

  "@subheading": {
    "prototype": "#fira-sans-bold",
    "font": {
      "size": "25"
    },
    "colours": {
      "normal_text": "rgb(204, 204, 204)"
    }
  },

  "#start-game": {
    "prototype": "#fira-sans-bold",
    "colours": {
      "normal_bg": "hsl(180, 50%, 20%)",
      "normal_text": "hsl(240, 5%, 90%)",
      "hovered_bg": "hsl(180, 50%, 25%)",
      "hovered_text": "hsl(240, 0%, 100%)",
      "active_bg": "hsl(60, 80%, 20%)",
      "active_text": "hsl(60, 80%, 50%)",
      "active_border": "hsl(60, 80%, 50%)"
    },
    "font": {
      "size": "25"
    },
    "misc": {
      "shape": "rounded_rectangle",
      "shape_corner_radius": "12"
    }
  },
  "#start-shop": {
    "prototype": "#fira-sans-bold",
    "colours": {
      "normal_bg": "hsl(180, 50%, 20%)",
      "normal_text": "hsl(240, 5%, 90%)",
      "hovered_bg": "hsl(180, 50%, 25%)",
      "hovered_text": "hsl(240, 0%, 100%)",
      "active_bg": "hsl(60, 80%, 20%)",
      "active_text": "hsl(60, 80%, 50%)",
      "active_border": "hsl(60, 80%, 50%)"
    },
    "font": {
      "size": "25"
    },
    "misc": {
      "shape": "rounded_rectangle",
      "shape_corner_radius": "12"
    }
  },
  "#select-theme": {
    "prototype": "#fira-sans-bold",
    "colours": {
      "normal_bg": "hsl(180, 50%, 20%)",
      "dark_bg": "hsl(180, 50%, 20%)",
      "normal_text": "hsl(240, 0%, 100%)",
      "hovered_bg": "hsl(180, 50%, 25%)",
      "hovered_text": "hsl(240, 0%, 100%)",
      "active_bg": "hsl(60, 80%, 20%)",
      "active_text": "hsl(60, 80%, 50%)",
      "normal_border": "hsl(122, 11%, 40%)"
    },
    "font": {
      "size": "25"
    },
    "misc": {
      "shape": "rounded_rectangle",
      "shape_corner_radius": "12"
    }
  },
  "drop_down_menu.button": {
    "prototype": "#fira-sans-bold",
    "colours": {
      "normal_bg": "hsl(180, 50%, 20%)",
      "active_bg": "hsl(60, 80%, 20%)",
      "hovered_bg": "hsl(180, 50%, 25%)",
      "hovered_text": "hsl(240, 0%, 100%)"
    },
    "misc": {
      "shape": "rounded_rectangle",
      "shape_corner_radius": "12"
    },
    "font": {
      "size": "20"
    }
  },
  "#return": {
    "prototype": "#fira-sans-bold",
    "colours": {
      "normal_bg": "hsl(180, 50%, 20%)",
      "normal_text": "hsl(240, 5%, 90%)",
      "hovered_bg": "hsl(180, 50%, 25%)",
      "hovered_text": "hsl(240, 0%, 100%)",
      "active_bg": "hsl(60, 80%, 20%)",
      "active_text": "hsl(60, 80%, 50%)",
      "active_border": "hsl(60, 80%, 50%)"
    },
    "font": {
      "size": "25"
    },
    "misc": {
      "shape": "rounded_rectangle",
      "shape_corner_radius": "12"
    }
  },
  "#pause-panel": {
    "colours": {
      "normal_bg": "hsl(240, 10%, 10%)",
      "normal_border": "hsl(240, 10%, 10%)"
    },
    "misc": {
      "shape": "rounded_rectangle",
      "shape_corner_radius": "20"
    }
  }
}